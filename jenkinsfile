pipeline {
	agent any
	
	stages{
		stage('compile stage'){
		
		steps {
			withMaven(maven : 'maven_3_6_2'){
				sh 'mvn clean compile'
			  }
		}

}

		stage ('testing stage){
	
			steps {
			winMaven(maven : 'maven_3_6_2'){
				sh 'mvn test'
				}
		}
}
stage ('Deployment stage){
	
			steps {
			winMaven(maven : 'maven_3_6_2'){
				sh 'mvn deploy'
				}
		}
}

}
}